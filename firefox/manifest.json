{
    "manifest_version": 2,
    "name": "findplus",
    "version": "1.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["findplus.js", "js/jquery-3.1.1.js"]
        }
    ],

    "background": {
        "scripts": ["background-test.js", "js/compromise.js"]
    }
}
