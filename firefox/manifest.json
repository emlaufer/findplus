{
    "manifest_version": 2,
    "name": "findplus",
    "version": "1.0",

    "description": "Adds a red border to all webpages matching mozilla.org.",

    "icons": {
        "48": "icons/border-48.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery-3.3.1.js", "findplus.js"]
        }
    ],

    "background": {
        "scripts": ["js/compromise.js"]
    }
}
	
